{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "es2017"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx", "components/ServiceDetail/Navigation.js", "components/ServiceDetail/Refund.js", "components/ServiceDetail/Review.js", "components/ServiceDetail/Summary.js", "components/user/ErrorMessage.js", "components/user/Oauth.js", "components/user/Oauth.js", "components/user/SigninForm.js", "components/user/SignupAuthForm.js", "components/user/SigninForm"]
}
